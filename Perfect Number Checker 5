import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int temp=N;
        int sum=0;
        for(int i=1;i<N;i++){
            if(N%i==0){
                sum=sum+i;
            }
        }
        if(sum==temp){
            System.out.println("Perfect Number");
        }
        else{
            System.out.println("Not a Perfect Number");
        }
    }
}
