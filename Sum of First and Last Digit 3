import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int a,b;
        if(N<0)
            N=N*-1;
            a=N%10;
            while(N>=10){
               N=N/10; 
            }
            b=N;
            int sum=a+b;
            System.out.println(sum);
        
        
    }
}
