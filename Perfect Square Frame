import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int r=sc.nextInt();
        for(int i=0;i<r;i++){
            for(int j=0;j<r;j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
