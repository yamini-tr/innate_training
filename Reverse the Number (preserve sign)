import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int r=0;
        int N=sc.nextInt();
        if(N<0){
            System.out.print("-");
            N = N * -1;
        }
         if(N==0)
             r=0;
             while(N!=0){
            r=r*10+(N%10);
            N=N/10;
            
        }
        System.out.print(r);
}
}
