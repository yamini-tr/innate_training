import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int count=0;
        int num=sc.nextInt();
        if(num==0)
        {
            count=1;
           
        }
        else{
            
        while(num!=0){
            
            count++;
            
            num=num/10;
        }}
      System.out.println(count);      
            
    }
}
