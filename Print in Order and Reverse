import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();

        // Print 1 to N
        for (int i = 1; i <= N; i++) {
            System.out.print(i + " ");
        }

        // Print N to 1
        for (int i = N; i >= 1; i--) {
            System.out.print(i + " ");
        }
    }
}

