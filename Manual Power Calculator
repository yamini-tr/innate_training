import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int b=sc.nextInt();
        int e=sc.nextInt();
        int res =1;
        for(int i=1;i<=e;i++)
            res = res * b;
        System.out.println(res);
}
    }
    
